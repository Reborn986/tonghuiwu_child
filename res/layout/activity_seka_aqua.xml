<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/blue_li"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#E5EDF3">



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="#101130"
        android:id="@+id/toolbar_photo"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        app:title="@null">

        <ImageButton
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="#00FAFBED"
            android:id="@+id/imb_back"
            android:scaleType="fitXY"
            android:layout_marginLeft="20dp"
            android:src="@drawable/ic_back"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="35dp"
            android:textColor="@color/white"
            android:gravity="center"
            android:textSize="30dp"
            android:fontFamily="@font/hanyixuejun"
            android:text="中国传统颜色"/>
        <View
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"/>
        <ImageButton
            android:id="@+id/ic_more"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_gravity="end|center_vertical"
            android:background="#00FBF8B1"
            android:src="@drawable/ic_more2"
            android:layout_marginRight="10dp"
            android:scaleType="fitXY">

        </ImageButton>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:orientation="horizontal">
        <ImageView
            android:id="@+id/aqua_im"
            android:layout_width="125dp"
            android:layout_marginRight="5dp"
            android:layout_height="200dp"/>
        <LinearLayout
            android:id="@+id/picture"
            android:layout_width="wrap_content"
            android:layout_height="200dp"
            android:orientation="horizontal"
            android:layout_gravity="center"
            android:background="@drawable/bg_dianjiseka" >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="20dp"
                android:text="竹外桃花三两枝,\n春江水暖鸭先知"
                android:textSize="20dp"
                android:lineSpacingExtra="10dp"
                android:id="@+id/t_shiju"
                android:textColor="#ffffff"
                android:fontFamily="@font/hanyixuejun"
                android:layout_gravity="center_vertical"
                android:visibility="invisible"/>
        </LinearLayout>

    </LinearLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="wrap_content"
            android:orientation="vertical"
            android:id="@+id/table_view"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content">
            <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <ImageButton
                        android:id="@+id/button_shenhuilan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_shenhuilan"
                        android:background="@color/lightyellow"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_qingtinglan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_qingtinglan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_yuanweilan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_yuanweilan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_qinglan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_qinglan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_shanlan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_shanlan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_pubulan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_pubulan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_hushuilan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_hushuilan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>


                </LinearLayout>
            </HorizontalScrollView>
            <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="5dp">
                    <ImageButton
                        android:id="@+id/button_huilan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_huilan"
                        android:background="@color/lightyellow"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_tianlan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_tianlan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>

                    <ImageButton
                        android:id="@+id/button_xiakeqing"
                        android:layout_width="50dp"
                        android:layout_height="120dp"
                        android:layout_marginLeft="5dp"
                        android:background="@color/lightyellow"
                        android:scaleType="fitXY"
                        android:src="@drawable/aqua_xiakeqing" />

                    <ImageButton
                        android:id="@+id/button_danfanlan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_danfanlan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_dianzilan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_dianzilan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_qingshanlan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/auqa_qingshanlan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>

                    <ImageButton
                        android:id="@+id/button_oulan"
                        android:layout_width="50dp"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="5dp"
                        android:background="@color/lightyellow"
                        android:scaleType="fitXY"
                        android:src="@drawable/aqua_oulan" />

                </LinearLayout>
            </HorizontalScrollView>


            <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp">
                    <ImageButton
                        android:id="@+id/button_canglan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_canglan"
                        android:background="@color/lightyellow"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_chaolan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_chaolan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_yunshanlan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_yunshanlan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_haiqing"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_haiqing"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_jianshilan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_jianshilan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_jiqing"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_jiqing"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_jingtianlan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_jingtianlan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                </LinearLayout>
            </HorizontalScrollView>

            <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp">
                    <ImageButton
                        android:id="@+id/button_tangcilan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_tangcilan"
                        android:background="@color/lightyellow"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_youlan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_youlan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_jianniaolan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_jianniaolan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_kongquelan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_kongquelan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_bingshanlan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_bingshanlan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_qiubolan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_qiubolan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_haitianlan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/auqa_haitianlan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                </LinearLayout>
            </HorizontalScrollView>
            <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp">
                    <ImageButton
                        android:id="@+id/button_yuqinlan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_yuqinlan"
                        android:background="@color/lightyellow"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_baoshilan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_baoshilan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_cuilan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_cuilan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_biqing"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_biqing"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_weilan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_weilan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_qingshuilan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_qingshuilan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_yuantianlan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_yuantianlan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                </LinearLayout>
            </HorizontalScrollView>
            <HorizontalScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginTop="5dp">
                    <ImageButton
                        android:id="@+id/button_qianniuhualan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_qianniuhualan"
                        android:background="@color/lightyellow"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_wanbolan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_wanbolan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>
                    <ImageButton
                        android:id="@+id/button_gulan"
                        android:layout_height="120dp"
                        android:layout_width="50dp"
                        android:src="@drawable/aqua_gulan"
                        android:background="@color/lightyellow"
                        android:layout_marginLeft="5dp"
                        android:scaleType="fitXY"/>

                </LinearLayout>
            </HorizontalScrollView>

        </LinearLayout>
    </ScrollView>


</LinearLayout>