<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/yellow2">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:src="@drawable/aipainting_im2"
        android:scaleType="fitXY"
        android:background="@color/yellow2"/>
    <FrameLayout
        android:id="@+id/frame3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom">
        <ImageView
            android:id="@+id/bottom_im"
            android:layout_width="match_parent"
            android:layout_height="150dp"
            android:src="@drawable/main_im_bottom2"
            android:scaleType="fitXY"/>
        <LinearLayout
            android:id="@+id/bottom_linear"
            android:layout_height="40dp"
            android:layout_width="match_parent"
            android:layout_gravity="bottom"
            android:background="#A7B4CE"
            android:layout_marginBottom="2dp"
            android:orientation="horizontal">
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <ImageButton
                    android:id="@+id/imbutton_seka"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/ic_wodeseka2"
                    android:background="@android:color/transparent"
                    android:paddingTop="4dp"
                    android:paddingBottom="16dp"
                    android:scaleType="fitCenter" />
                <TextView
                    android:id="@+id/textbutton_seka"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="我的色卡"
                    android:textColor="#677695"
                    android:textSize="11dp"
                    android:layout_marginBottom="1dp"
                    android:layout_gravity="bottom|center_horizontal"/>
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <ImageButton
                    android:id="@+id/imbutton_home"
                    android:paddingTop="3dp"
                    android:paddingBottom="14dp"
                    android:layout_width="match_parent"
                    android:background="@android:color/transparent"
                    android:layout_weight="1"
                    android:src="@drawable/ic_house2"
                    android:scaleType="fitCenter"
                    android:layout_height="match_parent"/>

                <TextView
                    android:id="@+id/textbutton_home"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="主页"
                    android:textColor="#677695"
                    android:textSize="11dp"
                    android:layout_marginBottom="1dp"
                    android:layout_gravity="bottom|center_horizontal"/>
            </FrameLayout>
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="match_parent">
                <ImageButton
                    android:id="@+id/imbutton_shequ"
                    android:paddingTop="3dp"
                    android:paddingBottom="14dp"
                    android:layout_width="match_parent"
                    android:background="@android:color/transparent"
                    android:layout_weight="1"
                    android:src="@drawable/ic_shequ2"
                    android:scaleType="fitCenter"
                    android:layout_height="match_parent"/>

                <TextView
                    android:id="@+id/textbutton_shequ"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="社区交流"
                    android:textColor="#677695"
                    android:textSize="11dp"
                    android:layout_marginBottom="1dp"
                    android:layout_gravity="bottom|center_horizontal"/>
            </FrameLayout>


        </LinearLayout>
    </FrameLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="110dp"
        tools:context=".PhotoProcess">
<TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="图生图 参考图片"
    android:textStyle="bold"
    android:layout_gravity="right"
    android:layout_marginRight="10dp"
    android:textColor="#677694"/>
        <ImageView
            android:id="@+id/picture"
            android:layout_width="match_parent"
            android:layout_height="300dp"
            android:layout_gravity="center"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/aipainting_bg_im2"
            android:contentDescription="TODO" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="文生图 提示词"
            android:textStyle="bold"
            android:layout_gravity="right"
            android:layout_marginRight="10dp"
            android:textColor="#677694"/>
        <EditText
            android:id="@+id/prompt"
            android:layout_width="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:hint="请输入你想生成的图片"
            android:paddingLeft="10dp"
            android:textSize="13sp"
            android:background="@drawable/blue2_circular"
            android:layout_height="60dp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="98dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal">
            <ImageButton
                android:id="@+id/button_shot2"
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:paddingTop="15dp"
                android:paddingBottom="10dp"
                android:layout_marginLeft="5dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                android:scaleType="fitCenter"
                android:background="@drawable/aipainting_bg_qita2"
                android:elevation="4dp"
                android:src="@drawable/aipainting_im_paizhao2" />
            <androidx.appcompat.widget.AppCompatButton
                android:layout_width="match_parent"
                android:id="@+id/queren"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="10dp"
                android:layout_weight="1.3"
                android:background="@drawable/blue2_point"
                android:text="确 认"
                android:textColor="#677694"/>
            <ImageButton
                android:id="@+id/button_import2"
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:paddingTop="15dp"
                android:paddingBottom="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                android:scaleType="fitCenter"
                android:elevation="4dp"
                android:src="@drawable/aipainting_im_xiangce2"
                android:background="@drawable/aipainting_bg_qita2"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/love"
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="5dp"
                android:layout_weight="1.3"
                android:background="@drawable/blue2_point"
                android:text="收 藏 图 片"
                android:textColor="#677694"/>
            <androidx.appcompat.widget.AppCompatButton
                android:layout_width="match_parent"
                android:id="@+id/save"
                android:layout_height="35dp"
                android:layout_marginLeft="10dp"
                android:layout_marginBottom="5dp"
                android:layout_weight="1.3"
                android:background="@drawable/blue2_point"
                android:text="保 存 图 片"
                android:textColor="#677694"/>
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:text="AI  绘  画"
        android:layout_marginTop="25dp"
        android:textSize="20dp"
        android:textColor="#677695"/>

    <ImageButton
        android:id="@+id/back_button"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="30dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/photoprocess_ic_back2" />



</FrameLayout>