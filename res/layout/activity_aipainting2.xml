<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/yellow2">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="180dp"
        android:src="@drawable/aipainting_im_title"
        android:scaleType="fitXY"
        android:background="@color/yellow2"/>
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:layout_gravity="bottom"
        android:background="@drawable/main_im_bottom" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_marginLeft="15dp"
        android:layout_marginRight="15dp"
        android:layout_marginTop="110dp"
        tools:context=".PhotoProcess">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="图生图 参考图片"
            android:textStyle="bold"
            android:layout_gravity="right"
            android:layout_marginRight="10dp"
            android:textColor="#A48F8F"/>
        <ImageView
            android:id="@+id/picture"
            android:layout_width="match_parent"
            android:layout_height="300dp"
            android:layout_gravity="center"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="@drawable/aipainting_bg_im"
            android:contentDescription="TODO" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="文生图 提示词"
            android:textStyle="bold"
            android:layout_gravity="right"
            android:layout_marginRight="10dp"
            android:textColor="#AA9594"/>
        <EditText
            android:id="@+id/prompt"
            android:layout_width="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:hint="请输入你想生成的图片"
            android:paddingLeft="10dp"
            android:textSize="13sp"
            android:background="@drawable/pink3_circular"
            android:layout_height="60dp"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="98dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal">


            <ImageButton
                android:id="@+id/button_shot2"
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:layout_marginLeft="5dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1"
                android:background="@drawable/aipainting_bg_qita"
                android:elevation="4dp"
                android:paddingTop="15dp"
                android:paddingBottom="10dp"
                android:scaleType="fitCenter"
                android:src="@drawable/aipainting_im_paizhao" />
            <androidx.appcompat.widget.AppCompatButton
                android:layout_width="match_parent"
                android:id="@+id/queren"
                android:layout_height="65dp"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="10dp"

                android:layout_weight="1.3"
                android:background="@drawable/pink3_point"
                android:text="确 认"
                android:textColor="#A48F8F"/>

            <ImageButton
                android:id="@+id/button_import2"
                android:layout_width="match_parent"
                android:layout_height="75dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="5dp"
                android:layout_weight="1"
                android:background="@drawable/aipainting_bg_qita"
                android:elevation="4dp"
                android:paddingTop="15dp"
                android:paddingBottom="10dp"
                android:scaleType="fitCenter"
                android:src="@drawable/aipainting_im_xiangce" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:id="@+id/love"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="5dp"
                android:layout_weight="1.3"
                android:background="@drawable/pink3_point"
                android:text="收 藏 图 片"
                android:textColor="#A48F8F"/>
            <androidx.appcompat.widget.AppCompatButton
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:layout_marginLeft="10dp"
                android:id="@+id/save"
                android:layout_marginBottom="5dp"
                android:layout_weight="1.3"
                android:background="@drawable/pink3_point"
                android:text="保 存 图 片"
                android:textColor="#A48F8F"/>
        </LinearLayout>
    </LinearLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="25dp"
        android:text="AI  绘  画"
        android:textColor="@color/lightbrown"
        android:textSize="20dp" />

    <ImageButton
        android:id="@+id/back_button"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="30dp"
        android:background="@android:color/transparent"
        android:scaleType="fitXY"
        android:src="@drawable/photoprocess_ic_back" />

    <LinearLayout
        android:layout_height="40dp"
        android:layout_width="match_parent"
        android:layout_gravity="bottom"
        android:layout_marginBottom="2dp"
        android:orientation="horizontal">
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="match_parent">
            <ImageButton
                android:id="@+id/imbutton_seka"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:src="@drawable/ic_wodeseka"
                android:background="@color/pink"
                android:paddingTop="4dp"
                android:paddingBottom="16dp"
                android:scaleType="fitCenter" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="我的色卡"
                android:textColor="@color/lightbrown"
                android:textSize="11dp"
                android:layout_marginBottom="1dp"
                android:layout_gravity="bottom|center_horizontal"/>
        </FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="match_parent">
            <ImageButton
                android:id="@+id/imbutton_home"
                android:paddingTop="3dp"
                android:paddingBottom="14dp"
                android:layout_width="match_parent"
                android:background="@color/pink"
                android:layout_weight="1"
                android:src="@drawable/ic_house"
                android:scaleType="fitCenter"
                android:layout_height="match_parent"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="主页"
                android:textColor="@color/lightbrown"
                android:textSize="11dp"
                android:layout_marginBottom="1dp"
                android:layout_gravity="bottom|center_horizontal"/>
        </FrameLayout>
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="match_parent">
            <ImageButton
                android:id="@+id/imbutton_shequ"
                android:paddingTop="3dp"
                android:paddingBottom="14dp"
                android:layout_width="match_parent"
                android:background="@color/pink"
                android:layout_weight="1"
                android:src="@drawable/ic_shequ"
                android:scaleType="fitCenter"
                android:layout_height="match_parent"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="社区交流"
                android:textColor="@color/lightbrown"
                android:textSize="11dp"
                android:layout_marginBottom="1dp"
                android:layout_gravity="bottom|center_horizontal"/>
        </FrameLayout>


    </LinearLayout>

</FrameLayout>